generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ContactForm {
  id               String   @id @default(uuid()) @db.Uuid
  createdAt        DateTime @default(now()) @db.Timestamp(6)
  name             String
  email            String
  companyName      String
  companySize      String
  notes            String
  projectTypes     String[]
  engineeringTypes String[]
  otherServices    String[]
}
